    SELECT
		office.OFFICE_ID,
		office.OFFICE_CODE,
	    office.OFFICE_NAME,
		office.IS_DELETE,
		zone.ZONE_CODE,
		zone.ZONE_NAME
	FROM
		system_office office
	LEFT JOIN system_zone zone ON office.ZONE_CODE=zone.ZONE_CODE
	WHERE
		office.IS_DELETE=0
  </select>
  
  <select id="queryOfficeByUser" resultMap="BaseResultMap" parameterType="map">
  	SELECT OFF.OFFICE_ID,OFF.OFFICE_CODE,OFF.OFFICE_NAME,OFF.IS_DELETE,OFF.ZONE_CODE FROM system_office OFF
	LEFT JOIN system_station STA ON STA.OFFICE_CODE=OFF.OFFICE_CODE AND OFF.IS_DELETE = 0
	LEFT JOIN security_user_station SUS ON SUS.STATION_CODE=STA.STATION_CODE
	WHERE SUS.USER_ID=#{userId,jdbcType=VARCHAR}
